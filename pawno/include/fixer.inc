			        }
			    }
			}
		}
		else if (((newkeys & (KEY_AIM)) != (KEY_AIM)) && ((oldkeys & (KEY_AIM)) == (KEY_AIM)))
		{
		    for (new i; i < MAX_PLAYER_ATTACHED_OBJECTS; i++)
			{
				if (!IsPlayerAttachedObjectSlotUsed(playerid, i) && FIXER_p_Attachment[playerid][i][FIXER_e_PLAYER_ATT_USED])
