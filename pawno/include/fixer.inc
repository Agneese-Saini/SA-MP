											 	FIXER_p_Attachment[playerid][i][FIXER_e_PLAYER_ATT_SX], FIXER_p_Attachment[playerid][i][FIXER_e_PLAYER_ATT_SY], FIXER_p_Attachment[playerid][i][FIXER_e_PLAYER_ATT_SZ],
