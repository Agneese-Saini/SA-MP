	#undef OnPlayerUpdate
#else
	#define _ALS_OnPlayerUpdate
#endif
#define OnPlayerUpdate FIXER_OnPlayerUpdate
#if defined FIXER_OnPlayerUpdate
	forward FIXER_OnPlayerUpdate(playerid);
#endif

